audi_s7
	=> nrel_main_idtf: [Модельный ряд Ауди S 7] (* <-lang_ru ;;*);
=> nrel_main_idtf: [Car lineup Audi S 7] (* <-lang_en;; *);
	=> nrel_generation:
		audi_s7_4G;
		audi_s7_4G_restyling;
		audi_s7_4K;;

audi_s7_4G
	=> nrel_main_idtf: [Ауди S 7 4G] (* <-lang_ru;; *);
	=> nrel_main_idtf: [Audi S 7 4G] (* <-lang_en;; *);
		<- concept_car;
	=> nrel_type_of_engine:
		concept_car_gasoline_engine;
	=> nrel_type_of_transmission:
		concept_car_robotic_transmission;
	=> nrel_type_of_front_suspension:
    		concept_car_McPherson_suspension;
  	=> nrel_type_of_rear_suspension:
    		concept_car_multi_link_suspension;
	=> nrel_type_of_body:
		concept_car_liftback_body;
	=> nrel_type_of_drive:
		concept_car_four_wheel_drive;
	=> nrel_period:...
	(*
		<- sc_node_tuple;;
		-> rrel_start_year: 2012;;
		-> rrel_end_year: 2014;; 
	*);
	=> nrel_photo: "file://img/audi_s7_4G.png" (* => nrel_format: format_png;; *);; 


