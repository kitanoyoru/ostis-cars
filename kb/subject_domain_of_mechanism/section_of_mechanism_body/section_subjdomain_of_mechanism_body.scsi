	<= nrel_subdividing: 	
		{  
			concept_car_sedan_body;
		    concept_car_hatchback_body;
		    concept_car_station_wagon_body;
		    concept_car_liftback_body;
		    concept_car_coupe_body;
		    concept_car_cabriolet_body;
		    concept_car_roadster_body;
		    concept_car_targa_body;
            concept_car_SUV_body;
            concept_car_crossover_body;
            concept_car_pickup_body;
            concept_car_minivan_body;
		};
	<- rrel_key_sc_element:
		...
		(*
			<- explanation;;
			<= nrel_sc_text_translation:
			...
			(*
			-> rrel_example:
					"file://content_html/explanation_for_body.html"			
				(*
				<- lang_ru;;
				=> nrel_format: format_html;;
				*);;
			*);;
		*);;

