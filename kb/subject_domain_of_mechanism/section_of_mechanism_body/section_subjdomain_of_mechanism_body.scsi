subject_domain_of_cars_mechanism_body
	=> nrel_main_idtf: 
		[Предметная область кузова] (* <- lang_ru;; *);
		[Subject domain of body] (* <- lang_en;; *);

	<- subject_domain;
	<- sc_node_struct;
	<- concept_car_body;
	-> rrel_maximum_studied_object_class: 
		concept_car_body;
	-> rrel_not_maximum_studied_object_class:
		concept_car_sedan_body;
		concept_car_hatchback_body;
		concept_car_station_wagon_body;
		concept_car_liftback_body;
		concept_car_coupe_body;
		concept_car_cabriolet_body;
		concept_car_roadster_body;
		concept_car_targa_body;
        concept_car_SUV_body;
        concept_car_crossover_body;
        concept_car_pickup_body;
        concept_car_minivan_body;
	-> rrel_explored_relation:
		nrel_type_of_body;;

concept_car_body
	<- sc_node_not_relation;
	=> nrel_main_idtf: 
		[Кузов автомобиля] (* <- lang_ru;; *);
