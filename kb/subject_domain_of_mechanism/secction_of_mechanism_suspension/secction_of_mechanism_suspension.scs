	<- sc_node_norole_relation;
	=> nrel_first_domain: concept_car;
	=> nrel_second_domain: concept_car_suspension;
	=> nrel_main_idtf: [Тип передней подвески*] (* <- lang_ru;; *);
	=> nrel_main_idtf: [Type of front suspension*](* <- lang_en;; *);;

